CREATE TABLE NTRN_PRECHARGE_HEADER (
	SHOP_CODE VARCHAR(7) NOT NULL,
	SALE_DATE VARCHAR(8) NOT NULL,
	POS_NO VARCHAR(2) NOT NULL,
	CST_NO VARCHAR(10) NOT NULL,
	SALE_NO VARCHAR(4) NOT NULL,
	REGI_SEQ VARCHAR(2) DEFAULT '01' NOT NULL,
	CHARGE_YN VARCHAR(1) DEFAULT 'Y' NOT NULL,
	PAY_TYPE_FLAG VARCHAR(2) DEFAULT '0' NOT NULL,
	CHARGE_AMT NUMERIC(12,2) DEFAULT '0' NOT NULL,
	CHARGE_REM_AMT NUMERIC(12,2) DEFAULT '0' NOT NULL,
	EMP_NO VARCHAR(4),
	ORG_SALE_NO VARCHAR(30),
	RTN_MSG_CODE VARCHAR(100),
	RTN_MSG VARCHAR(500),
	EMP_NO VARCHAR(1) NOT NULL,
	SEND_FLAG VARCHAR(1) DEFAULT '0',
	SEND_DT VARCHAR(14),
	INSERT_DT VARCHAR(14)

	CONSTRAINT NTRN_PRECHARGE_HEADER_PL PRIMARY KEY (SHOP_CODE,SALE_DATE,POS_NO,CST_NO,SALE_NO)
);

COMMENT ON TABLE NTRN_PRECHARGE_HEADER IS '비매출 - 선결제 충전 Header TRAN';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.SHOP_CODE IS '매장코드';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.SALE_DATE IS '영업일자';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.POS_NO IS '포스번호';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.CST_NO IS '회원번호';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.SALE_NO IS '선결제충전영수증번호';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.REGI_SEQ IS '정산차수';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.CHARGE_YN IS '충전여부';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.PAY_TYPE_FLAG IS '결제수단구분';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.CHARGE_AMT IS '충전금액';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.CHARGE_REM_AMT IS '충전후잔액';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.EMP_NO IS '판매원번호';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.ORG_SALE_NO IS '반품-원거래영수번호';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.RTN_MSG_CODE IS '반품-사유코드';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.RTN_MSG IS '반품-사유';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.EMP_NO IS '판매원코드';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.SEND_FLAG IS '서버전송구분';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.SEND_DT IS '서버전송일시';
COMMENT ON COLUMN NTRN_PRECHARGE_HEADER.INSERT_DT IS '등록일시';



CREATE TABLE NTRN_PRECHARGE_CARD (
	SHOP_CODE VARCHAR(7) NOT NULL,
	SALE_DATE VARCHAR(8) NOT NULL,
	POS_NO VARCHAR(2) NOT NULL,
	CST_NO VARCHAR(10) NOT NULL,
	SALE_NO VARCHAR(4) NOT NULL,
	REGI_SEQ VARCHAR(2) DEFAULT '01' NOT NULL,
	CHARGE_YN VARCHAR(1) DEFAULT 'Y' NOT NULL,
	PAY_TYPE_FLAG VARCHAR(2) DEFAULT '0' NOT NULL,
	CHARGE_AMT NUMERIC(12,2) DEFAULT '0' NOT NULL,
	CHARGE_REM_AMT NUMERIC(12,2) DEFAULT '0' NOT NULL,
	EMP_NO VARCHAR(4),
	ORG_SALE_NO VARCHAR(30),
	RTN_MSG_CODE VARCHAR(100),
	RTN_MSG VARCHAR(500),
	EMP_NO VARCHAR(1) NOT NULL,
	SEND_FLAG VARCHAR(1) DEFAULT '0',
	SEND_DT VARCHAR(14),
	INSERT_DT VARCHAR(14),

	CONSTRAINT NTRN_PRECHARGE_HEADER_PL PRIMARY KEY (SHOP_CODE,SALE_DATE,POS_NO,CST_NO,SALE_NO)
);

CREATE TABLE NTRN_PRECHARGE_CARD (
	SHOP_CODE VARCHAR(7) NOT NULL,
	SALE_DATE VARCHAR(8) NOT NULL,
	POS_NO VARCHAR(2) NOT NULL,
	CST_NO VARCHAR(10) NOT NULL,
	SALE_NO VARCHAR(4) NOT NULL,
	REGI_SEQ VARCHAR(2) DEFAULT '01',
	CHARGE_YN VARCHAR(1) DEFAULT 'Y' NOT NULL,
	APPR_PROC_FLAG VARCHAR(1),
	CRD_CARD_NO VARCHAR(20),
	VALID_TERM VARCHAR(6),
	APPR_REQ_AMT NUMERIC(12,2) DEFAULT '0',
	APPR_AMT NUMERIC(12,2) DEFAULT '0',
	APPR_DC_AMT NUMERIC(12,2) DEFAULT '0',
	SVC_TIP_AMT NUMERIC(12,2) DEFAULT '0',
	VAT_AMT NUMERIC(12,2) DEFAULT '0',
	INST_MM_FLAG VARCHAR(1),
	INST_MM_CNT NUMERIC(2,0) DEFAULT '0',
	SIGN_PAD_YN	VARCHAR(1),
	CARD_IN_FLAG VARCHAR(1),
	APPR_FLAG VARCHAR(1),
	APPR_DATE VARCHAR(8),
	APPR_TIME VARCHAR(6),
	APPR_NO VARCHAR(20),
	APPR_MSG VARCHAR(200),
	VAN_CODE VARCHAR(2),
	CRDCP_CODE VARCHAR(3),
	ISS_CRDCP_CODE VARCHAR(10),
	ISS_CRDCP_NAME VARCHAR(40),
	PUR_CRDCP_CODE VARCHAR(4),
	PUR_CRDCP_NAME VARCHAR(40),
	VAN_TERM_NO VARCHAR(16),
	VAN_SLIP_NO VARCHAR(12),
	CRDCP_TERM_NO VARCHAR(15),
	ORG_APPR_DATE VARCHAR(8),
	ORG_APPR_NO VARCHAR(20),
	APPR_LOG_NO VARCHAR(5)
	INSERT_DT VARCHAR(14),
	CONSTRAINT NTRN_PRECHARGE_CARD_PK PRIMARY KEY (SHOP_CODE,SALE_DATE,POS_NO,CST_NO,SALE_NO)
);

COMMENT ON TABLE NTRN_PRECHARGE_CARD IS '비매출 - 선결제충전 신용카드 TRAN';
COMMENT ON COLUMN NTRN_PRECHARGE_CARD.SHOP_CODE	IS '매장코드';
