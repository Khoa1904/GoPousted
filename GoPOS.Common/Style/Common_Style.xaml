<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="Grid" x:Key="baseGrid">
        <Setter Property="Width" Value="1024" />
        <Setter Property="Height" Value="768" />
        <Setter Property="Background" Value="#b4b4b4" />
    </Style>
    <Style TargetType="DockPanel" x:Key="subGrid">
        <Setter Property="Background" Value="#ebebeb" />
    </Style>

    <Style x:Key="listView1" TargetType="{x:Type ListView}">
        <Setter Property="FontWeight" Value="Bold" />
        <Setter Property="FontFamily" Value="Pretendard" />
        <Setter Property="Foreground" Value="#FF000000" />
        <Setter Property="Height" Value="27"/>
        <Setter Property="Margin" Value="-2,-1,0,0" />
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="{x:Type GridViewColumnHeader}">
                    <Border BorderThickness="0,0,1,1" BorderBrush="#bababa" Margin="0,0,0,0">
                        <TextBlock x:Name="ContentHeader" Text="{TemplateBinding Content}" Padding="0,5,0,0" Width="{TemplateBinding Width}" TextAlignment="Center" VerticalAlignment="Center" Background="#f5f5f5" Height="27" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    <Style x:Key="TransparentBorderStyle" TargetType="{x:Type TextBox}">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="TextBox">
                    <Border BorderBrush="White" BorderThickness="1,1,1,0" Padding="2">
                        <TextBlock Text="{TemplateBinding Text}" Foreground="{TemplateBinding Foreground}" />
                    </Border>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
