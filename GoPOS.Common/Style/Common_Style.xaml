    <Style TargetType="Button" x:Key="btnBstyle">
        <Setter Property="Width" Value="100" />
        <Setter Property="Height" Value="50" />
        <Setter Property="Background" Value="Green" />
        <Setter Property="Content" Value="B스타일" />
